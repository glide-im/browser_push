{
  "name": "GlidePush",
  "description": "Glide Browser Message Push",
  "version": "1.0",
  "manifest_version": 3,
  "background": {
    "service_worker": "init.js"
  },
  "permissions": ["storage"],
  "action": {
    "default_popup": "config.html",
    "default_icon": {
      "16": "images/16.png",
      "32": "images/32.png",
      "48": "images/48.png",
      "128": "images/128.png"
    }
  },
  "icons": {
    "16": "images/16.png",
    "32": "images/32.png",
    "48": "images/48.png",
    "128": "images/128.png"
  }
}